{
  "name": "jspredict-dc",
  "version": "2.1.0",
  "description": "javascript port of predict open-source satellite tracking library",
  "main": "dist/jspredict-dc.cjs.js",
  "module": "dist/jspredict-dc.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/fanqie/jspredict-dc"
  },
