    "moment": "^2.24.0",
    "satellite.js": "^3.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "rollup": "^4.41.1",
    "rollup-plugin-dts": "^6.2.1"
  },
  "scripts": {
